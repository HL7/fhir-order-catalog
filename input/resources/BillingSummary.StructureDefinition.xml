      <max value="1" />
    </element>
    <element id="Extension.extension">
      <path value="Extension.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Extension.extension:textualSummary">
      <!--  
      <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
